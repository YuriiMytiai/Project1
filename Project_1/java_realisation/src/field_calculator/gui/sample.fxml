                        <TextField fx:id="phiSourceField" layoutX="237.0" layoutY="312.0" prefHeight="10.0" prefWidth="41.0" text="0" visible="false" />
                        <TextField fx:id="thetaSourceField" layoutX="237.0" layoutY="350.0" prefHeight="10.0" prefWidth="41.0" text="0" visible="false" />
                        <TextField fx:id="gainSourceField" layoutX="237.0" layoutY="387.0" prefHeight="10.0" prefWidth="41.0" text="0" visible="false" />
