                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Calculations">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Pane fx:id="chart3" layoutX="156.0" layoutY="11.0" prefHeight="460.0" prefWidth="653.0" />
                        <ChoiceBox fx:id="freqChoise" layoutX="29.0" layoutY="103.0" prefHeight="25.0" prefWidth="97.0" />
                        <Label layoutX="50.0" layoutY="76.0" text="Frequency" />
                        <Button fx:id="singleSourceCalcBut" layoutX="34.0" layoutY="173.0" mnemonicParsing="false" onAction="#singleSourceCalcButCallback" text="Single Source" />
                        <Button fx:id="sumFieldCalcBut" layoutX="30.0" layoutY="245.0" mnemonicParsing="false" onAction="#sumFieldCalcButCallback" text="Summary Filed" />
                     </children>
